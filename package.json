        "rimraf": "^3.0.2"
    },
    "engines": {
        "node": "14.16.1"
    },
    "browserslist": [
        "last 1 Chrome versions"
    ],
    "scripts": {
        "deploy:testnet": "npm run build && gh-pages -d dist",
        "preinstall": "npx npm-force-resolutions",
        "deploy": "REACT_APP_ENV=\"prod\" yarn build && cp -a static/* dist && gh-pages -d dist",
        "prebuild": "npm run clean",
        "build": "node --max-old-space-size=8192 ./node_modules/.bin/parcel build src/index.html --no-scope-hoist",
        "clean": "rimraf ./dist && rimraf .parcel-cache",
        "fix": "eslint src/ --fix",
        "startup": "npm run clean && env-cmd -f ./.env parcel src/index.html --public-url ./ --no-source-maps",
        "startup1": "parcel src/index.html --no-source-maps --no-hmr",
        "startup2": "npm run clean && node --max-old-space-size=8192 ./node_modules/.bin/parcel src/index.html --no-source-maps",
        "startup3": "node --max-old-space-size=8192 ./node_modules/.bin/parcel src/index.html --no-source-maps",
        "start": "node server.js"
    }
}
