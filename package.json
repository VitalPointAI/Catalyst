        "parcel": "^2.0.0-beta.2",
        "rimraf": "^3.0.2"
    },
    "jest": {
        "modulePathIgnorePatterns": [
            "examples/.*",
            "packages/.*/build"
        ],
        "collectCoverageFrom": [
            "**/packages/**/*.js",
            "!**/vendor/**",
            "!**/__mocks__/**",
            "!**/__tests__/**",
            "!integration_tests/**",
            "**/src/**/*.js"
        ],
        "coverageReporters": [
            "json"
        ]
    },
    "browserslist": [
        "last 1 Chrome versions"
    ],
    "context": "node",
    "engines" : {
        "node" : ">=16.0.0"
    },
    "scripts": {
        "test": "jest --coverage",
        "deploy:testnet": "npm run build && gh-pages -d dist",
        "deploy": "REACT_APP_ENV=\"prod\" yarn build && cp -a static/* dist && gh-pages -d dist",
        "prebuild": "npm run clean",
        "build": "node ./node_modules/.bin/parcel build src/index.html --no-scope-hoist",
        "build:local": "npm run clean && env-cmd -f ./.env.local node ./node_modules/.bin/parcel build src/index.html --no-scope-hoist --no-optimize",
        "build:main": "node ./node_modules/.bin/parcel build src/index.html --no-scope-hoist",
        "clean": "rimraf ./dist && rimraf .parcel-cache",
        "cleandist": "rimraf ./dist",
        "fix": "eslint src/ --fix",
        "startup": "npm run clean && env-cmd -f ./.env parcel src/index.html --public-url ./ --no-source-maps",
        "startup1": "parcel src/index.html --no-source-maps --no-hmr",
        "startup2": "npm run clean && node --max-old-space-size=8192 ./node_modules/.bin/env-cmd -f ./.env parcel src/index.html --no-source-maps",
        "startup3": "node --max-old-space-size=8192 ./node_modules/.bin/parcel src/index.html --no-source-maps",
        "start": "node server.js"
    }
}
